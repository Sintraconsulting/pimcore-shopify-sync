{
    "name": "sintra/pimcore-shopify-using-ipaas",
    "description": "A Pimcore's bundle for products, prices and translation sync using ipaas",
    "license": "GPL-3.0-or-later",
    "type": "pimcore-bundle",
   
  "config": {
    "discard-changes": true
  },
  "autoload": {
    "psr-4": {
      "Product\\": "./"
    },
    "exclude-from-classmap": [
      "vendor/phpunit"
    ]
  },
  "require": {
    "pimcore/pimcore": "^11.0"
  },
    "extra": {
        "pimcore": {
            "bundles": [
                "SyncShopifyBundle\\SyncShopifyBundle"
            ]
        }
    },
  "require-dev": {
    "symfony/phpunit-bridge": "*"
  }
}
